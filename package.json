{
  "name": "@momomemory/openclaw-momo",
  "version": "0.1.1",
  "type": "module",
  "description": "OpenClaw plugin that gives agents persistent memory using Momo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momomemory/openclaw-momo.git"
  },
  "keywords": [
    "openclaw",
    "momo",
    "memory",
    "plugin",
    "ai"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && cp openclaw.plugin.json dist/openclaw.plugin.json && tsc --emitDeclarationOnly",
    "check-types": "tsc --noEmit",
    "typecheck": "bun run check-types",
    "test": "bun test"
  },
  "dependencies": {
    "@momomemory/sdk": "^0.2.1",
    "@sinclair/typebox": "0.34.47"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.29"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "^1.0.0",
    "typescript": "^5.7.3"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md"
  ]
}
